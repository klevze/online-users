{
    "name": "klevze/online-users",
    "description": "Display online users on a webpage",
    "keywords": ["laravel", "online", "users", "count"],
    "homepage": "https://github.com/klevze/online-users",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Gregor KlevÅ¾e",
            "email": "gregor@klevze.si",
            "homepage": "https://thewallpapers.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10 || ^11 || ^12"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1 || ^11.5",
        "php-coveralls/php-coveralls": "^0.1.0",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0 || ^11.0 || ^12.0",
        "mockery/mockery": "^1.5",
        "phpstan/phpstan": "^1.10",
        "friendsofphp/php-cs-fixer": "^3.16",
        "doctrine/dbal": "^3.6"
    },
    
    "autoload": {
        "psr-4": {
            "Klevze\\OnlineUsers\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Klevze\\OnlineUsers\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Klevze\\OnlineUsers\\OnlineUsersServiceProvider"
            ],
            "aliases": {
            }
        }
    },
    "suggest": {
        "illuminate/support": "Required for laravel service providers"
    }

    ,"scripts": {
        "test": "vendor/bin/phpunit",
        "analyse": "vendor/bin/phpstan analyse",
        "fix-style": "vendor/bin/php-cs-fixer fix --dry-run --diff --stop-on-violation"
    }
}
